.row-fluid
  .span12
    %h3
      Task: #{@task.app.name}
    %p
      Completed at: #{@task.completed_at.nil? ? "Not Completed" : @task.completed_at.to_s }
    %ul
      - @task.task_items.each do |task_item|
        %li
          = link_to task_item.form.name, task_item.form.url, :target => "_blank"
          