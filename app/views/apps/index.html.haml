.row
  .twelve.columns
    %h3
      MyGov Applications  
  - @apps.each do |app|
    = render :partial => 'apps', :locals => { :app => app }

-  unless @sandbox_apps.empty?
  .row
    .twelve.columns
      %h3
        Sandbox Applications  
      - @sandbox_apps.each do |app|
        = render :partial => 'apps', :locals => { :app => app }
  
